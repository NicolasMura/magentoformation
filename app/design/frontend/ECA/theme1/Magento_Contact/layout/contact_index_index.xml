<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
  <body>
    <referenceContainer name="content">
      <block class="Magento\Framework\View\Element\Template" name="block1" template="block1.phtml" />
      <block class="Magento\Framework\View\Element\Template" name="block2" template="block2.phtml" before="contactForm" />
    </referenceContainer>

    <referenceContainer name="before.body.end">
      <block class="Magento\Framework\View\Element\Template"
        name="contact_alert.js"
        template="Magento_Contact::js_init.phtml" />
    </referenceContainer>
  </body>
</page>
